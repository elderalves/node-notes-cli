{
  "name": "notes-node",
  "version": "1.1.0-beta.0",
  "description": "",
  "main": "app.js",
  "dependencies": {
    "lodash": "^4.17.15",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^9.1.7",
    "prettier": "^1.19.1",
    "semantic-release": "^24.2.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "nextVersion": "1.1.0-beta.2"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elderalves/node-notes-cli.git"
  },
  "keywords": [
    "notes"
  ],
  "author": "Michael Alves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elderalves/node-notes-cli/issues"
  },
  "homepage": "https://github.com/elderalves/node-notes-cli#readme"
}
